<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>UID</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.uid %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
